#!/usr/bin/env ruby
# frozen_string_literal: true

require 'set'
require 'digest'

known = {}
Dir['*'].sort.reverse.each do |file|
  digest = Digest::SHA256.file(file).hexdigest
  if (dupe = known[digest])
    puts "#{file}: Duplicate of #{dupe}"
    File.unlink file
  else
    known[digest] = file
  end
end
